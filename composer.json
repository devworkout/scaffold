{
  "name": "devworkout/scaffold",
  "description": "",
  "keywords": [
    "devworkout",
    "scaffold"
  ],
  "homepage": "https://github.com/devworkout/scaffold",
  "license": "MIT",
  "authors": [
    {
      "name": "Vladislav Novik",
      "email": "librevlad@gmail.com",
      "homepage": "https://serpentine.io",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "afterflow/framework": "dev-master",
    "devworkout/scaffold-docker": "dev-master",
    "guzzlehttp/guzzle": "^6.3",
    "nunomaduro/laravel-console-task": "^1.2",
    "toin0u/digitalocean-v2": "^2.3"
  },
  "require-dev": {
    "laravel/framework": "5.*",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "DevWorkout\\Scaffold\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DevWorkout\\Scaffold\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "DevWorkout\\Scaffold\\ScaffoldServiceProvider"
      ]
    }
  },
  "repositories": {
    "local-/work/afterflow/": {
      "type": "path",
      "url": "/work/afterflow/"
    }
  }
}
